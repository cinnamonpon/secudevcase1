<div class="page-header">
  <h1>Donations</h1>
</div>
<div class="row">
  <div class="col-lg-8">
    <table class="table table-striped table-hover">
      <thead>
        <th>Amount</th>
        <th>Status</th>
        <th></th>
      </thead>
      <tbody>
        <%= render @donations if @donations.any? %>
    </tbody>
    </table>
    <center><%= will_paginate @donations, renderer: BootstrapPagination::Rails%></center>

  </div>

  <div class="col-lg-4">
    <%= render 'shared/store_nav' %>
  </div>

</div>
